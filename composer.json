{
    "name": "xcrawler",
    "description": "toolkit to xml/html parser projects",
    "license": "Apache 2.0",
    "repositories": [
		{
			"type": "package",
			"package": {
				"name": "sergioska/xcrawler",
				"version": "dev-master",
				"source": {
					"type": "git",
					"url": "https://github.com/sergioska/xcrawler.git",
					"reference": "origin"
				}
			}
		}
    ],
    "autoload": {
		"psr-4": {"xcrawler\\": "src/xcrawler"}
    },
	"extra": {
		"installer-paths": {
				"libs": ["sergioska/xcrawler"]
			}
	},
    "require-dev": {
        "vimeo/psalm": "^3.11"
    },
	"require": {
		"ext-dom": "*",
		"ext-curl": "*",
		"webmozart/assert": "^1.8"
	}
}
